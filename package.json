{
    "name": "ibcm",
    "displayName": "ibcm",
    "description": "Basic language support for the IBCM machine language",
    "publisher": "hanzhi713",
    "version": "0.0.4",
    "repository": {
        "type": "git",
        "url": "https://github.com/hanzhi713/vscode-ibcm.git"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:ibcm"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "ibcm",
                "aliases": [
                    "IBCM",
                    "ibcm"
                ],
                "extensions": [
                    ".ibcm"
                ]
            }
        ],
        "grammars": [
            {
                "language": "ibcm",
                "scopeName": "source.ibcm",
                "path": "./syntaxes/ibcm.tmGrammar.json"
            }
        ],
        "snippets": [
            {
                "language": "ibcm",
                "path": "./snippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "tslint": "^5.12.1",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}
